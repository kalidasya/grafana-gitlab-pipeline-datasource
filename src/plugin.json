{
  "type": "datasource",
  "name": "Gitlab Pipelines Datasource",
  "id": "grafana-gitlab-pipelines-datasource",
  "metrics": true,
  "info": {
    "description": "",
    "author": {
      "name": "tkende",
      "url": ""
    },
    "keywords": [],
    "logos": {
      "small": "img/logo.svg",
      "large": "img/logo.svg"
    },
    "links": [],
    "screenshots": [],
    "version": "%VERSION%",
    "updated": "%TODAY%"
  },
  "dependencies": {
    "grafanaVersion": "6.5.x",
    "plugins": []
  },
  "routes": [
    {
      "path": "",
      "url": "{{.JsonData.url}}",
      "headers": [
        {
          "name": "Authorization",
          "content": "Bearer {{ .SecureJsonData.privateToken }}"
        },
        {
          "name": "Content-Type",
          "content": "application/json"
        }
      ]
    }
  ]
}